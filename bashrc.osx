# Set default editor
export EDITOR=/usr/bin/vim

# Homebrew: Use local versions of executables first
export PATH="/usr/local/bin:$PATH"

# Terminal enhanced
alias cp='cp -iv'
alias mv='mv -iv'
alias mkdir='mkdir -pv'
alias ll='ls -FGlAhp'

# Golang
export GOPATH=$HOME/go:$HOME/git/kwms-server
export GOROOT=/usr/local/go
export PATH=$GOPATH/bin:$PATH
export PATH=$GOROOT/bin:$PATH

# Oracle InstantClient
export PATH=$HOME/programs/instantclient_12_2:$PATH
